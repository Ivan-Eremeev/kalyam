									ul.filters__checklist 
										li 	
											span.filters__check 
												input#filter_check_1(type="radio" name="checklist" checked)
												label(for="filter_check_1") Сначала дешёвые
												label(for="filter_check_1").filters__checkbox
										li
											span.filters__check 
												input#filter_check_2(type="radio" name="checklist")
												label(for="filter_check_2") Сначала дорогие
												label(for="filter_check_2").filters__checkbox
										li
											span.filters__check 
												input#filter_check_3(type="radio" name="checklist")
												label(for="filter_check_3") Лучшие предложения
												label(for="filter_check_3").filters__checkbox
										li
											span.filters__check 
												input#filter_check_4(type="radio" name="checklist")
												label(for="filter_check_4") Товары со скидкой
												label(for="filter_check_4").filters__checkbox
									.filters__views
										.filters__view#viewTile.active
											<svg width="21" height="22" viewBox="0 0 21 22" fill="none" xmlns="http://www.w3.org/2000/svg">
												<rect x="0.75" y="0.75" width="8.42533" height="8.42533" rx="1.25" stroke-width="1.5"  fill="none"/>
												<rect x="0.75" y="11.8281" width="8.42533" height="8.42533" rx="1.25" stroke-width="1.5"  fill="none"/>
